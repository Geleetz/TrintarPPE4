{{ form_start(form) }}
<div class="row">
    <div class="col-md-3">{{ form_row(form.horaireOuverture) }}</div>
    <div class="col-md-3">{{ form_row(form.adresse) }}</div>
    <div class="col-md-3">{{ form_row(form.lng) }}</div>
    <div class="col-md-3">{{ form_row(form.lat) }}</div>
</div>
<div class="row">
    <div class="col-md-4">{{ form_row(form.ville) }}</div>
    <div class="col-md-4">{{ form_row(form.departement) }}</div>
    <div class="col-md-4">{{ form_row(form.pays) }}</div>
</div>
<div class="row">
    <div class="col-md-3">{{ form_row(form.nom) }}</div>
    <div class="col-md-3">{{ form_row(form.telephone) }}</div>
    <div class="col-md-3">{{ form_row(form.courriel) }}</div>
    <div class="col-md-3">{{ form_row(form.imageFile) }}</div>
</div>
{{ form_rest(form) }}
<button class="btn btn-primary">{{ button|default('Enregistrer') }}</button>
{{ form_end(form) }}
<br>